<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/rectangle"
            android:layout_width="344dp"
            android:layout_height="579dp"
            android:layout_marginTop="72dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/rectangle" />

        <ImageView
            android:id="@+id/userButton"
            android:layout_width="96dp"
            android:layout_height="96dp"
            android:layout_marginStart="64dp"
            android:layout_marginTop="232dp"
            app:layout_constraintStart_toStartOf="@+id/rectangle"
            app:layout_constraintTop_toTopOf="@+id/rectangle"
            app:srcCompat="@drawable/userbutton" />

        <ImageView
            android:id="@+id/userLogo"
            android:layout_width="220dp"
            android:layout_height="200dp"
            android:layout_marginTop="36dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/userlogo" />

        <ImageView
            android:id="@+id/logoutButton"
            android:layout_width="48dp"
            android:layout_height="0dp"
            android:layout_marginTop="356dp"
            app:layout_constraintEnd_toEndOf="@+id/rectangle"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="@+id/rectangle"
            app:layout_constraintTop_toBottomOf="@+id/userLogo"
            app:srcCompat="@drawable/logouticon" />

        <ImageView
            android:id="@+id/uploadButton"
            android:layout_width="96dp"
            android:layout_height="96dp"
            android:layout_marginTop="232dp"
            android:layout_marginEnd="60dp"
            app:layout_constraintEnd_toEndOf="@+id/rectangle"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/userButton"
            app:layout_constraintTop_toTopOf="@+id/rectangle"
            app:srcCompat="@drawable/uploadbutton" />

        <ImageView
            android:id="@+id/consultButton"
            android:layout_width="96dp"
            android:layout_height="96dp"
            android:layout_marginStart="68dp"
            android:layout_marginTop="32dp"
            app:layout_constraintStart_toStartOf="@+id/rectangle"
            app:layout_constraintTop_toBottomOf="@+id/userButton"
            app:srcCompat="@drawable/consultbutton" />

        <ImageView
            android:id="@+id/ubiButton"
            android:layout_width="96dp"
            android:layout_height="96dp"
            android:layout_marginTop="32dp"
            app:layout_constraintEnd_toEndOf="@+id/rectangle"
            app:layout_constraintHorizontal_bias="0.25"
            app:layout_constraintStart_toEndOf="@+id/consultButton"
            app:layout_constraintTop_toBottomOf="@+id/uploadButton"
            app:srcCompat="@drawable/ubibutton" />

        <TextView
            android:id="@+id/personalDataTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="68dp"
            android:layout_marginTop="4dp"
            android:fontFamily="@font/inter"
            android:text="@string/personal_data"
            android:textColor="@color/text"
            app:layout_constraintStart_toStartOf="@+id/rectangle"
            app:layout_constraintTop_toBottomOf="@+id/userButton" />

        <TextView
            android:id="@+id/uploadTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="68dp"
            android:fontFamily="@font/inter"
            android:text="@string/upload_book"
            android:textColor="@color/text"
            app:layout_constraintEnd_toEndOf="@+id/rectangle"
            app:layout_constraintTop_toBottomOf="@+id/uploadButton" />

        <TextView
            android:id="@+id/consultTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="72dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/inter"
            android:text="@string/check_books"
            android:textColor="@color/text"
            app:layout_constraintStart_toStartOf="@+id/rectangle"
            app:layout_constraintTop_toBottomOf="@+id/consultButton" />

        <TextView
            android:id="@+id/ubiTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="84dp"
            android:fontFamily="@font/inter"
            android:text="@string/location"
            android:textColor="@color/text"
            app:layout_constraintEnd_toEndOf="@+id/rectangle"
            app:layout_constraintTop_toBottomOf="@+id/ubiButton" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>